{
  "name": "cypress_udemy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "triggerAllTests-headless": "npx cypress run",
    "triggerAllTests-headed": "npx cypress run --headed",
    "triggerAllTests-chrome": "npx cypress run --browser chrome",   
    "triggerAllTests-firefox": "npx cypress run --browser firefox",
    "triggerAllTests-dashboard": "npx cypress run --record --key 56d9a6cd-8a02-4c9c-88dc-57f550b079fe",   
    "triggerAllTests-autostore-dashboard": "npx cypress run --spec cypress/e2e/automation-test-store/* --record --key 56d9a6cd-8a02-4c9c-88dc-57f550b079fe", 
    "triggerAllTests-autoTestStore": "npx cypress run --spec cypress/e2e/automation-test-store/*",    
    "triggerAllTests-webdriveruni": "npx cypress run --spec cypress/e2e/webDriverUnit/*",
    "junit-merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
    "mochawesome-merge": "npx mochawesome-merge cypress/results/mochawesome/*.json > mochawesome.json and npx marge mochawesome.json",
    "triggerAllTests-staging": "npx cypress run --env configFile=staging",
    "cypress-multi-browser": "npm run triggerAllTests-autoTestStore -- --browser electron --headed & npm run triggerAllTests-autoTestStore -- --browser edge --headed"
 
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^10.7.0"
  },
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress-mochawesome-reporter": "^3.7.0",
    "cypress-multi-reporters": "^1.6.4",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
